  "scripts": {
    "setup": "yarn install && yarn run floweditor",
    "start": "react-scripts start",
    "build": "npm run floweditor && react-scripts build",
    "test": "react-scripts test",
    "test:coverage": "react-scripts test --coverage --watchAll",
    "cy:run": "cypress run",
    "eject": "react-scripts eject",
    "format": "prettier \"**/*.{ts,tsx,css,graphql}\" --write",
    "floweditor": "rm -rf public/static && rm -rf public/sitestatic && cp -r node_modules/@nyaruka/flow-editor/build/static public && cp -r node_modules/@nyaruka/flow-editor/build/sitestatic public"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!src/config/*.{ts,tsx}",
      "!src/serviceWorker.ts",
      "!src/react-app-env.d.ts",
      "!src/index.tsx",
      "!src/types/**/*.{ts,tsx}"
    ],
    "coverageReporters": [
      "text",
      "html",
      "lcov"
    ]
  },
